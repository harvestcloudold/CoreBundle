{% extends 'HarvestCloudCoreBundle::Profile/layout.html.twig' %}
{% form_theme form 'HarvestCloudCoreBundle:Form:fields.html.twig' %}

{% block content %}
    <h1>Edit {{ location }}</h1>
    <form action="{{ path('Profile_location_update', { 'id' : location.id }) }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <div class="form row">
            <input type="submit" class="submit button" value="Save" />
        </div><!-- .form.row -->
    </form>
{% endblock %}
